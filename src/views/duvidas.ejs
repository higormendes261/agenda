<%- include('includes/head') %>
<%- include('includes/nav') %>

<head><link rel="stylesheet" type="text/css" href="/assets/css/duvidas.css" />
</head>

<section class="container-duvidas">
    <div>
        <img src="/assets/img/conversation 1.png" alt="">
        <h1>Dúvidas</h1>
    </div>

    <form class="formulario-duvidas" action="/" id="formulario-duvidas" method="POST">
        <label for="nome-duvidas">Nome:</label>
        <input type="text" id="nome-duvidas" name="nome-duvidas">

        <label for="email-duvidas">Email:</label>
        <input type="email" id="email-duvidas" name="email-duvidas" placeholder="exemplo@exemplo.com">

        <label for="descricao-duvidas">Descrição:</label>
        <textarea name="descricao-duvidas" id="descricao-duvidas" cols="30" rows="10" placeholder="Descreva sua dúvida aqui."></textarea>


        <button class="enviar-duvidas" type="submit">Enviar</button>

    </form>
</section>

<%- include('includes/footer') %>